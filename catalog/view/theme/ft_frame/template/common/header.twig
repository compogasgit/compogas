			<div class="preloader"><div class="bounce1"></div><div class="bounce2"></div><div class="bounce3"></div></div>
			{% endif %}
		</div>
	</div>
	<script>function remove_preloader(){$.when($(".preloader-wrapper").fadeOut(100)).then(function(e,r,o){$(".preloader-wrapper").remove(),$("body").removeClass("preloader_on")})}window.onload=remove_preloader,setTimeout(remove_preloader,{{ preloader_timeout }})</script>
</div>
{% endif %}
