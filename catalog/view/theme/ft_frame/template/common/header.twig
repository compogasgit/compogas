			<div class="preloader"></div>
			{% elseif preloader_type == 1 %}
			<div class="preloader"><div class="double-bounce1"></div><div class="double-bounce2"></div></div>
			{% elseif preloader_type == 2 %}
			<div class="preloader"><div class="rect1"></div> <div class="rect2"></div> <div class="rect3"></div> <div class="rect4"></div> <div class="rect5"></div></div>
			{% elseif preloader_type == 3 %}
